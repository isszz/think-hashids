{
    "name": "isszz/think-hashids",
    "description": "thinkphp Hashids",
    "keywords": [
        "think",
        "thinkphp",
        "hashids"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "isszz",
            "email": "isszz@qq.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "hashids/hashids": "^4.1.0",
        "topthink/framework": "8.*|6.0.*"
    },
    "autoload": {
        "psr-4": {
            "isszz\\hashids\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "dev-dev"
        },
        "think": {
            "config":{
                "hashids": "src/config/config.php"
            }
        }
    }
}